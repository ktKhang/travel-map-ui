  "scripts": {
    "watch": "./node_modules/.bin/webpack --watch --config webpack.config.js",
    "start:dev": "webpack-dev-server --mode development --config webpack.config.js --open --hot --history-api-fallback",
    "build": "webpack -p --mode development --config webpack.config.js --progress --colors",
    "build:prod": "webpack -p --mode production --config webpack.prod.config.js --progress --colors",
    "build-css": "node-sass-chokidar --include-path ./node_modules ./src/scss -o ./src/scss",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./node_modules ./src/scss -o ./src/scss --watch --recursive",
    "start-js": "react-scripts start",
    "startnowp": "npm-run-all -p watch-css start-js",
    "start": "webpack-dev-server  --mode production --config webpack.prod.config.js --open --hot --history-api-fallback"
  },
